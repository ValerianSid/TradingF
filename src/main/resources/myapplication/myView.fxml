<?import javafx.scene.layout.VBox?>

<VBox prefHeight="230.0" prefWidth="430.0" spacing="10.0" xmlns="http://javafx.com/javafx/16"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapplication.MyViewController">
<padding>
    <Insets bottom="10.0" left="10.0" top="10.0"/>
</padding>
<Button onAction="#currencyById" text="Currency by ID">
    <graphic>
        <TextField prefHeight="25.0" prefWidth="80.0"/>
    </graphic>
</Button>
<Label fx:id="labelText"/>
</VBox>